    "babel-tape-runner": "^2.0.1",
    "browserify": "^14.4.0",
    "chalk": "^2.1.0",
    "fs-extra": "^4.0.1",
    "jsdoc": "^3.5.4",
    "jsdoc-template": "github:ircam-jstools/jsdoc-template",
    "klaw": "2.0.0",
    "np": "^2.16.0",
